{
	"name": "FRI3NDZ.Angular4Template",
	"version": "1.0.0",
	"private": true,
	"description": "An Angular VS template.",
	"main": "wwwroot/index.html",
	"scripts": {
		"start": "npm run webpack-dev && webpack-dev-server --env=dev --hot --inline --port 9999",
		"webpack-dev": "rimraf wwwroot/* && webpack --env=dev",
		"webpack-production": "rimraf wwwroot/* && webpack --env=prod",
		"build-dev": "npm run webpack-dev",
		"build-production": "npm run webpack-production",
		"watch-webpack-dev": "rimraf wwwroot/* && webpack --env=dev --watch --color",
		"watch-webpack-production": "npm run build-production --watch --color",
		"publish-for-iis": "npm run build-production && dotnet publish -c Release",
		"test": "karma start",
		"test-watch": "karma start --no-single-run",
		"lint": "tslint ./app"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
